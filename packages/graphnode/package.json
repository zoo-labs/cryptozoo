    "scripts": {
      "start": "cargo run -p graph-node --release -- --postgres-url postgresql://graph-node:let-me-in@localhost:5432/graph-node --ethereum-rpc mainnet:http://127.0.0.1:8545 --ipfs 127.0.0.1:5001",
